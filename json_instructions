//Post a product
{
    "name": "String",
    "price": "Number"
}
//Form-data enables to add a productImage as a file

//Patch a product
[
    {"propName" : "name", "value":  "String"},
    {"propName" : "price", "value": "Number"}
]

// Post an order
{
    "productID": "String",
    "quantity":  "Number" //Optional
}